{
  "name": "com-yingw787-cooking",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "pkg/src/apps/com-yingw787-cooking",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/pkg/src/apps/com-yingw787-cooking"
      },
      "configurations": {
        "development": {
          "outputPath": "pkg/src/apps/com-yingw787-cooking"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "com-yingw787-cooking:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "com-yingw787-cooking:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "com-yingw787-cooking:build:production",
          "dev": false
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "pkg/src/apps/com-yingw787-cooking/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "autoformat": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "pkg/src/apps/com-yingw787-cooking",
        "commands": [
          "npx prettier --config ../../../../.prettierrc.json --ignore-path ../../../../.prettierignore --write ."
        ],
        "parallel": true
      },
      "envFile": ".env.local"
    },
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "dependsOn": ["build"],
      "options": {
        "engine": "docker",
        "tags": ["com-yingw787-cooking:latest"]
      }
    }
  },
  "tags": []
}
