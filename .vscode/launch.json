{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "com-yingw787",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "nx",
        "serve",
        "com-yingw787",
        "--port",
        "5000",
        "--inspect"
      ],
      "envFile": "${workspaceFolder}/.env.local",
      "skipFiles": ["<node_internals>/**", "${workspaceFolder}/node_modules/*"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
