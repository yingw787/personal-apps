{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "com-yingw787",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "nx",
        "serve",
        "com-yingw787",
        "--port",
        "3000",
        "--inspect"
